Question 3
#!/bin/bash

if [ "$#" -ne 1 ];then
    echo "Usage: $0 <PID>"
    exit 1;
fi

for ((i = 0; i < 60; i++));do
    ps -o pid,ppid,state --pid $1
    sleep 1
done

